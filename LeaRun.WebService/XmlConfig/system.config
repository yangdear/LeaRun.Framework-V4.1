  <!-- 联系我们信息 -->
  <add key="Contact" value="QQ：100000;Email：100000@qq.com;" />
  <!-- 软件许可证编号 -->
  <add key="Licence" value="NO:2013101712345678" />
  <!-- ================== 4：系统邮件错误报告反馈相关 ================== -->
  <!-- 软件的错误报告发给谁 -->
  <add key="ErrorReportTo" value="403722956@qq.com,1020344464@qq.com" />
  <!-- 发出错误邮箱的地址 -->
  <add key="ErrorReportMailUserName" value="100000@qq.com" />
