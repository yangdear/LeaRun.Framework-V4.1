<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!-- ================== 1：数据库连接相关配置 ================== -->
  <!--默认数据库软件类型：Oracle,SqlServer,MySql,Access,SQLite-->
  <add key="ComponentDbType" value="SqlServer" />
  <!-- 连接字符串是否加密-->
  <add key="ConStringEncrypt" value="false" />
  <!-- ================== 2：这里主要是服务器端配置项内容部分 ================== -->
  <!-- 系统默认登录账户 -->
  <add key="CurrentUserName" value="System" />
